{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "api-types": {},
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["src/**"],
            "outputs": ["dist/**"]
        },
        "start": {
            "persistent": true,
            "cache": false,
            "dependsOn": ["^build"]
        }
    }
}